{
    "canisters": {
        "http_counter": {
            "type": "custom",
            "build": "python -m kybra http_counter src/main.py src/main.did",
            "candid": "src/main.did",
            "wasm": ".kybra/http_counter/http_counter.wasm",
            "gzip": true,
            "metadata": [
                {
                    "name": "candid:service",
                    "path": "src/main.did"
                },
                {
                    "name": "cdk:name",
                    "content": "kybra"
                }
            ],
            "declarations": {
                "output": "test/dfx_generated/http_counter",
                "node_compatibility": true
            }
        }
    }
}
