{
    "canisters": {
        "hello": {
            "type": "custom",
            "build": "python -m kybra hello src/main.py src/main.did",
            "candid": "src/main.did",
            "wasm": ".kybra/hello/hello.wasm",
            "gzip": true,
            "metadata": [
                {
                    "name": "candid:service",
                    "path": "src/main.did"
                },
                {
                    "name": "cdk:name",
                    "content": "kybra"
                }
            ],
            "declarations": {
                "output": "test/dfx_generated/hello",
                "node_compatibility": true
            }
        }
    }
}
