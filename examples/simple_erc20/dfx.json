{
    "canisters": {
        "simple_erc20": {
            "type": "custom",
            "build": "python -m kybra simple_erc20 src/main.py src/main.did",
            "candid": "src/main.did",
            "wasm": ".kybra/simple_erc20/simple_erc20.wasm",
            "gzip": true,
            "metadata": [
                {
                    "name": "candid:service",
                    "path": "src/main.did"
                },
                {
                    "name": "cdk:name",
                    "content": "kybra"
                }
            ],
            "declarations": {
                "output": "test/dfx_generated/simple_erc20",
                "node_compatibility": true
            }
        }
    }
}
